{
    "compileOnSave": true,
    "compilerOptions": {
      "allowSyntheticDefaultImports":true,
      "strict": false,
      "forceConsistentCasingInFileNames": true,
      "noFallthroughCasesInSwitch": true,
      "sourceMap": true,
      "moduleResolution": "node",
      "lib": ["dom", "es2015", "es2016"],
      "jsx": "react-jsx",
      "target": "es6",
    },
    "include": ["assets/**/*"]
}